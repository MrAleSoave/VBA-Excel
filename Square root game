Dim num0 As Variant
Dim num1 As Variant
Dim sqr0 As Variant

  num0 = InputBox("insert first number")
  num1 = InputBox("insert second number")
  sqr0 = sqr(num0)

If num0 = (num1 * num1) Then

  MsgBox ("yes the second number is the square root")
  
Else

  MsgBox ("no the square root is = " & sqr0)
  
End If
